


@if (Session["UserID"] == null || Session["usuario"] == null || Session["nombre"] == null ||
                            Session["apellido"] == null || Session["nomSede"] == null || Session["codSede"] == null)
{
    <html>
    <head>
        <meta http-equiv="Refresh" content="0;url=../Login">
    </head>
    <body style="display:none">
        @RenderBody()
    </body>
</html>

}
