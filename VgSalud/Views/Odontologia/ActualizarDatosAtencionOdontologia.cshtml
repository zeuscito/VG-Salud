<script>
    $(document).ready(function () {
        var Odontograma = new Array();

        $("#IdFractura").click(function () {
            $("#IdFractura").addClass('btn btn-danger');

            $("#IdRestauracion").removeClass('btn btn-info');
            $("#IdRestauracion").addClass('btn btn-default');

            $("#IdExtraccion").removeClass('btn btn-warning');
            $("#IdExtraccion").addClass('btn btn-default');

            $("#IdPuente").removeClass('btn btn-success');
            $("#IdPuente").addClass('btn btn-default');

            $("#IdBorrar").addClass('btn btn-default');

            $("#Accion").val('1');
        });

        $("#IdRestauracion").click(function () {
            $("#IdRestauracion").addClass('btn btn-info');

            $("#IdFractura").removeClass('btn btn-danger');
            $("#IdFractura").addClass('btn btn-default');

            $("#IdExtraccion").removeClass('btn btn-warning');
            $("#IdExtraccion").addClass('btn btn-default');

            $("#IdPuente").removeClass('btn btn-success');
            $("#IdPuente").addClass('btn btn-default');

            $("#IdBorrar").addClass('btn btn-default');

            $("#Accion").val('2');
        });

        $("#IdExtraccion").click(function () {
            $("#IdFractura").removeClass('btn btn-danger');
            $("#IdFractura").addClass('btn btn-default');

            $("#IdRestauracion").removeClass('btn btn-info');
            $("#IdRestauracion").addClass('btn btn-default');

            $("#IdExtraccion").addClass('btn btn-warning');

            $("#IdPuente").removeClass('btn btn-success');
            $("#IdPuente").addClass('btn btn-default');

            $("#Accion").val('3');
        });

        $("#IdPuente").click(function () {
            $("#IdFractura").removeClass('btn btn-danger');
            $("#IdFractura").addClass('btn btn-default');

            $("#IdRestauracion").removeClass('btn btn-info');
            $("#IdRestauracion").addClass('btn btn-default');

            $("#IdExtraccion").removeClass('btn btn-warning');
            $("#IdExtraccion").addClass('btn btn-default');

            $("#IdPuente").addClass('btn btn-success');

            $("#Accion").val('4');
        });

        $("#IdBorrar").click(function () {
            $("#IdFractura").removeClass('btn btn-danger');
            $("#IdFractura").addClass('btn btn-default');

            $("#IdRestauracion").removeClass('btn btn-info');
            $("#IdRestauracion").addClass('btn btn-default');

            $("#IdExtraccion").removeClass('btn btn-warning');
            $("#IdExtraccion").addClass('btn btn-default');

            $("#IdPuente").removeClass('btn btn-success');
            $("#IdPuente").addClass('btn btn-default');

            $("#Accion").val('5');
        });



        //******* CUADRANTE SUPERIOR DERECHO
        // --- DIENTE #18 ADULTO ---
        $("#D18LT").click(function () {
            var Accion = $("#Accion").val();

            if (Accion == '1') {
                $('#D18LT').css('background-color', '#dd4b39');
                var ValueHidden = 'D18LT' + '-' + Accion;
                $('#hiddenD18LT').val(ValueHidden);
                var ValorDato = $('#hiddenD18LT').val();
                Odontograma[0] = ValorDato;
                $('#ArregloOdontograma').val(Odontograma);

                var Dato=$('#ArregloOdontograma').val();
                alert("ESTADO ARREGLO: " + Dato);
            }

            if (Accion == '2') {
                $('#D18LT').css('background-color', '#00c0ef');
                var ValueHidden = 'D18LT' + '-' + Accion;
                $('#hiddenD18LT').val(ValueHidden);
                var ValorDato = $('#hiddenD18LT').val();
                Odontograma[0] = ValorDato;
                $('#ArregloOdontograma').val(Odontograma);

                var Dato = $('#ArregloOdontograma').val();
                alert("ESTADO ARREGLO: " + Dato);
