
@model VgSalud.Models.E_CSLaboratorio

@{
    var ListaAtendidos = (List<VgSalud.Models.E_CSLaboratorio>)ViewBag.Lista;
    var NumeroAtendido = 0;
}


<div class="content-wrapper">
    <section class="content-header">
        <h1>Laboratorio</h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-flask"></i>Laboratorio</a></li>
            <li class="active">Lista De Paciente Atendidos</li>
        </ol>
    </section>
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box">
                    <div class="row">
                        <div style="float:left;margin-top:1%;margin-left:1%">
                            <a href="~/Master/Index"><center style="margin-left:25px;"><i class="fa fa-home" style="font-size:25px; color:#13D003"></i><br /><strong style="color:black">Home</strong></center></a>
                            <br />
                        </div>
                    </div>

                    <fieldset>

                        <legend style="margin-left:3%;width:94%">Busqueda de Pacientes</legend>
                        <form method="post">
                            <div class="col-md-12">
                                <div class="col-md-2 col-md-offset-2">
                                    <label>Dni</label>
                                    <input type="text" name="NumDoc" class="form-control" value="@ViewBag.NumDoc" />
                                </div>
                                <div class="col-md-2">
                                    <label>Cod. Cue.</label>
                                    <input type="text" name="CodCue"  class="form-control" value="@ViewBag.CodCue" />
                                </div>
                                <div class="col-md-2">
                                    <label>Fecha Atencion:</label>
                                    <input type="text" class="form-control datepicker" name="FechaAten" value="@ViewBag.FechaAten" />
                                </div>
                                <div class="col-md-2">
                                    <br />
                                    <button type="submit" class="btn btn-warning"><span class="fa fa-search"></span> Buscar</button>
                                </div>

                            </div>
                        </form>

                        <legend style="margin-left:3%;width:96%">Lista Pacientes Atendidos en el Dia</legend>

                        <div class="box-body">
                            <div class="col-md-12 " style="padding-left:44px;padding-right: 115px;">
                                <table id="example1" class="table table-bordered table-striped">
                                    <thead>
                                        <tr class="headTabla">
                                            <th width="5%" style="text-align:center">Nro. Atendido</th>
                                            <th width="10%" style="text-align:center">Cod. Cue.</th>
                                            <th width="40%" style="text-align:center">Paciente</th>
                                            <th width="10%" style="text-align:center">TipoCarnet</th>
                                            <th width="10%" style="text-align:center">Manipulador</th>
                                            <th width="10%" style="text-align:center">Edad</th>
                                            <th width="10%" style="text-align:center">Apto</th>
                                            <th width="10%" style="text-align:center"><i class="fa fa-pencil-square-o"></i></th>

                                        </tr>
                                    </thead>
                                    <tbody>


                                        @foreach (var item in ListaAtendidos)
                                        {
                                            NumeroAtendido++;
                                                <tr>
                                                    <td style="text-align:center">@NumeroAtendido</td>
                                                    <td style="text-align:center">@item.CodCue</td>
                                                    <td>@Html.DisplayFor(modelItem => item.Paciente)</td>
                                                    <td>@Html.DisplayFor(modelItem => item.DesTipoCarnet)</td>
                                                    <td>@Html.DisplayFor(modelItem => item.Manipulador)</td>
                                                    <td style="text-align:center">@item.Edad</td>
                                                    <td style="text-align:center">@item.Apto</td>
                                                    <td style="text-align:center"><a title="Actualizar" class="btn btn-info" href="ModificarDatosPacientesAtendidosDelDia?IdLaboratorio=@item.Id" action='@Url.Action("ModificarDatosPacientesAtendidosDelDia", "Laboratorio")'><i class="fa fa-pencil-square-o"></i> Actualizar</a></td>


                                                </tr>

                                            }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </section>

</div>

<script>

    $(document).ready(function () {

    });

</script>











