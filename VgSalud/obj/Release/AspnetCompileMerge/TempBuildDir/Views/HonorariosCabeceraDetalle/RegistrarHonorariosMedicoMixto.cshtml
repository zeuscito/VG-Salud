<script>

    function marcarTodos() {
        if (document.formulario1.todos.checked) {
            for (i = 0; i < document.formulario1.length; i++) {
                if ((document.formulario1.elements[i].id) == "marca") {
                    document.formulario1.elements[i].checked = true;
                    $("#registrar").attr("disabled", false);
                }
            }
        }
        else {
            for (i = 0; i < document.formulario1.length; i++) {
                if ((document.formulario1.elements[i].id) == "marca") {
                    document.formulario1.elements[i].checked = false;
                    $("#registrar").attr("disabled", true);
                }
            }
        }

        sumar();
        sumarGroup();
    }


    function sumar() {
     
        obj = document.formulario1['arreglo'];
        totalChecks = obj.length;


        if (totalChecks == null) {
            var dame = $("#marca").val();
            valor = dame.split('|');
            $("#SumaTotal").val(parseFloat(valor[9]));
            $("#registrar").attr("disabled", true);
        }
        else {
            var totalSumado = 0.00;
            var indice = 0;
            for (i = 0; i < totalChecks; i++) {
                if (obj[i].checked == true) {
                    valor = obj[i].value.split('|');
                    totalSumado = totalSumado + parseFloat(valor[9]);
                    indice++;

                }
            }
            $("#SumaTotal").val(totalSumado.toFixed(2));
            var sumar = $("#SumaTotal").val();
            if (sumar == '0.00') {
                $("#registrar").attr("disabled", true);
            }
            else {
                $("#registrar").attr("disabled", false);
            }
        }
     
    }


    function sumarGroup() {
        obj = document.formulario1['arreglo'];

        totalChecks = obj.length;
        var codMed = "@ViewBag.medico";
        var totalTurno = 0;

        if (totalChecks == null) {
            $("#IdDemo1").val("0");
            $("#IdDemoT").val("0");
        }
        else {
            var concatena = "";
            $("#PagoxTurno").val("@ViewBag.PagoTurno");
            var tipoR = "@ViewBag.tipoRango";
            for (i = 0; i < totalChecks; i++) {
                if (obj[i].checked == true) {
                    valor = obj[i].value.split('|');
                    concatena = concatena + valor[20] + ",";
                }
            }
            $.get('@Url.Action("GroupVFinal", "HonorariosCabeceraDetalle")', { cadena: concatena, tipo: tipoR },
                function (data) {
                    $.each(data, function (index, value) {
                        $("#IdDemo").val(value.TotalTurnos);
                        $("#Cant").val(value.TotalTurnos);
                        var ddd = $("#IdDemo1").val();
                        var ddC = $("#PagoxTurno").val();
                        var tot = ddd * value.TotalTurnos;
                        var to = ddC * value.TotalTurnos;
                        $("#IdDemoT").val(tot);
                        $("#TotalTurno").val(to);
                    });
                });
        }
    }



    $(document).on("ready", function () {

        $("#SumaTotal").val('0.00');
        var sumar = $("#SumaTotal").val();
        if (sumar == '0.00') {
            $("#registrar").attr("disabled", true);
        }
        else {
            $("#registrar").attr("disabled", false);
        }
        var medico = "@ViewBag.medico";
        var TipoLiqui = "@ViewBag.TipoLiqui";

        if (TipoLiqui == 1) {
            $("#porPorcentaje").fadeIn();
            $("#porTurno").fadeOut();
            $("#PagoxTurno").val("@ViewBag.PagoTurno");

            $(".dd").change(function () {
                var concatena = "";
                var tipoR = "@ViewBag.tipoRango";
                obj = document.formulario1['arreglo'];
                totalChecks = obj.length;
                for (i = 0; i < totalChecks; i++) {
                    if (obj[i].checked == true) {
                        valor = obj[i].value.split('|');
                        concatena = concatena + valor[20] + ",";

                    }
                }
                $.get('@Url.Action("GroupVFinal", "HonorariosCabeceraDetalle")', { cadena: concatena, tipo: tipoR },
                function (data) {
                    $.each(data, function (index, value) {
                        $("#Cant").val(value.TotalTurnos);
                        var ddC = $("#PagoxTurno").val();
                        var to = ddC * value.TotalTurnos;
                        $("#TotalTurno").val(to);
                    });
                });
            });

        }

        if (TipoLiqui == 2) {
            $("#porPorcentaje").fadeOut();
            $("#porTurno").fadeIn();
            $("#IdDemo1").val("@ViewBag.PagoTurno");
