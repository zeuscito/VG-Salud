@model VgSalud.Models.E_Usuario

@{
    ViewBag.Title = "RegistraUsuario";
}


<script type="text/javascript">

    $(document).ready(function () {

        $("#DniUsu").keydown(function (event) {
            if (event.shiftKey) {
                event.preventDefault();
            }

            if (event.keyCode == 46 || event.keyCode == 8) {
            }
            else {
                if (event.keyCode < 95) {
                    if (event.keyCode < 48 || event.keyCode > 57) {
                        event.preventDefault();
                    }
                }
                else {
                    if (event.keyCode < 96 || event.keyCode > 105) {
                        event.preventDefault();
                    }
                }
            }
        });
    }); 


</script>


<div class="content-wrapper">
    <section class="content-header">
        <h1>Especialidades</h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-users"></i>Usuario</a></li>
            <li class="active">Registrar Usuario</li>
        </ol>
    </section>
    <section class="content">
        <div class="box box-default">
            <div class="box-header with-border">
                <h3 class="box-title">Registrar Usuario</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                    <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
                </div>
            </div>

            <form method="post" action="">

                <div class="box-body">
                    <div class="col-md-6">

                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })




                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Alias</label>
                            </div>
                            <div class="col-md-9">
                               
                                    @Html.EditorFor(model => model.aliasUsu, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.aliasUsu, "", new { @class = "text-danger" })
                               
                            </div>

                        </div> 
                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Password</label>
                            </div>
                            <div class="col-md-9">

                                @Html.PasswordFor(model => model.pwdUsu, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.pwdUsu, "", new { @class = "text-danger" })

                            </div>

                        </div> 


                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Dni</label>
                            </div>
                            <div class="col-md-9">
                                @Html.EditorFor(model => model.DniUsu, new { htmlAttributes = new { @class = "form-control" , @id="DniUsu" } })
                                @Html.ValidationMessageFor(model => model.DniUsu, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Apellido Paterno</label>
                            </div>
                            <div class="col-md-9">
                                @Html.EditorFor(model => model.ApePaterUsu, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.ApePaterUsu, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Apellido Materno</label>
                            </div>
                            <div class="col-md-9">
                                @Html.EditorFor(model => model.ApeMaterUsu, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.ApeMaterUsu, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Nombre Usuario</label>
                            </div>
                            <div class="col-md-9">
                              
                                @Html.EditorFor(model => model.NomUsu, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.NomUsu, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Fecha Nacimiento</label>
                            </div>
                            <div class="col-md-9">
                                
                                <input type="text" name="FecNacUsu" id="FecNacUsu" maxlength="10" class="form-control  datepicker"  />
                                @Html.ValidationMessageFor(model => model.FecNacUsu, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Estado</label>
                            </div>
                            <div class="col-md-9">
                                @Html.CheckBoxFor(model => model.EstUsu, new { @checked = "checked" })
                                @Html.ValidationMessageFor(model => model.EstUsu, "", new { @class = "text-danger" })
                            </div>
                        </div>

                      


                    </div> 
                    <div class="col-md-6"></div>


                    <label></label>
                    <br /><br />
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="col-md-6">
                                <center>
                                    <button type="submit" value="Modificar" class="btn btn-danger">Registrar</button>
                                    <a href="#" onclick="history.back();return false" class="btn btn-info">Cancelar</a>
                                </center>
                            </div>
                        </div>
                    </div>
                </div>

            </form>

        </div>
    </section>
</div>

