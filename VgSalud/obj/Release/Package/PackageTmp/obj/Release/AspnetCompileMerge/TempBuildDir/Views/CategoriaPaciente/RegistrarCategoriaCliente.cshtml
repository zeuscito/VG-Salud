@model VgSalud.Models.E_Categoria_Paciente

@{
    ViewBag.Title = "RegistrarCategoriaCliente";
    var listadoSede = (SelectList)ViewBag.ListaSedes;
}


<div class="content-wrapper">
    <section class="content-header">
        <h1>Categoria pacientes</h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-users"></i>Categorias de paciente</a></li>
            <li class="active">Registrar Categoria de paciente</li>
        </ol>
    </section>
    <section class="content">
        <div class="box box-default">
            <div class="box-header with-border">
                <h3 class="box-title">Registrar Categoria de paciente</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                    <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
                </div>
            </div>

            <form method="post" action="">

                <div class="box-body">
                    <div class="col-md-6">

                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Categoria de paciente</label>
                            </div>
                            <div class="col-md-9">
                                @Html.EditorFor(model => model.DescCatPac, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.DescCatPac, "", new { @class = "text-danger" })
                            </div>
                        </div>
                         
                        <div class="form-group">
                            <div class="col-md-3">
                                <label>Estado</label>
                            </div>
                            <div class="col-md-9">
                                @Html.CheckBoxFor(model => model.EstCatPac, new { @checked = "checked" })
                                @Html.ValidationMessageFor(model => model.EstCatPac, "", new { @class = "text-danger" })
                            </div>
                        </div>


                    </div>
                    <div class="col-md-6">




                    </div>


                    <label></label>
                    <br /><br />
                    <div class="col-md-12">
                        <div class="col-md-3">
                            
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">

                                <center>
                                    <button type="submit" value="Registrar" class="btn btn-danger">Registrar</button>
                                    <a href="#" onclick="history.back();return false" class="btn btn-info">Cancelar</a>
                                </center>
                            </div>
                        </div>

                        @*<div class="col-md-3">
                            <div class="form-group">

                                <center>
                                    <a href="#" onclick="history.back();return false" class="btn btn-info">Cancelar</a>
                                </center>
                            </div>
                        </div>*@
                    </div>



                </div>
            </form>
        </div>
    </section>
</div>