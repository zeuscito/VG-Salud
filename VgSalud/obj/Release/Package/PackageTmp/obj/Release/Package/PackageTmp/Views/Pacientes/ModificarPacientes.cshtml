                    if (e.keyCode == ALT) {
                        presiono1 = true;
                    }
                    if (presiono1 == true && (e.keyCode == 123) && ver == true) {
                        $("#modificar").click();
                    }
                    else if ((presiono1 == true) && (e.keyCode == 65)) {
