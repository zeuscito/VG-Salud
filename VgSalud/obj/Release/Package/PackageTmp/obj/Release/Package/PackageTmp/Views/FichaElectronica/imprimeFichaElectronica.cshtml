@{
    Layout = null;

    var listaFe = (List<VgSalud.Models.E_Ficha_Electronica>)ViewBag.listaFe;
    var listaExamen = (List<VgSalud.Models.E_Ficha_Electronica>)ViewBag.listaExamen;
    var listaReceta = (List<VgSalud.Models.E_Ficha_Electronica>)ViewBag.listaReceta;
    var listaDiagnostico = (List<VgSalud.Models.E_Ficha_Electronica>)ViewBag.listaDiagnostico;

}

<!DOCTYPE html>




<html>
<head>
    <title> </title>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <style type="text/css">
        body, td, th {
            font-size: 8px;
            font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
        }

        table {
            border-collapse: collapse;
        }

        th {
            -webkit-print-color-adjust: exact;
            background-color: #0099FF;
            color: #006;
        }

        .Tit1 {
            font-style: italic;
            font-variant: small-caps;
            font-weight: bolder;
            color: #006;
        }
    </style>
    <script>
        
        function redireccionarPagina() {
            window.location = "../BandejaDeAtenciones";
        }
        setTimeout("redireccionarPagina()", 1000);
    </script>

</head>
<body leftmargin="0" topmargin="0" marginwidth="0" marginheight="0" onload="window.print()">

    <div id="Area">

        @foreach (var fe in listaFe)
{


        <table width="100%" border="0" cellpadding="1" cellspacing="0">
            <tr>
                <th height="20" colspan="5" align="center">FILIACION</th>
            </tr>
            <tr>
                <td height="20" colspan="3" align="left"><span class="Tit1">Paciente :</span> @fe.Nombre</td>
                <td width="10%" align="right" class="Tit1">N&deg; Historia :</td>
                <td width="15%">@fe.Historia</td>
            </tr>
            <tr>
                <td width="25%" height="20"><span class="Tit1">Sexo :</span> @fe.sexo</td>
                <td width="25%"><span class="Tit1">Edad :</span> @fe.edad A&Ntilde;OS</td>
                <td width="25%"><span class="Tit1">DNI: </span>@fe.numDoc</td>
                <td align="right" class="Tit1">Cuenta / Item :</td>
                <td>@fe.CodCue / @fe.Item</td>
            </tr>
            <tr>
                <td height="20"><span class="Tit1">Fecha / Hora :</span> @fe.FecRegMed 5:33 P.M.</td>
                <td colspan="4"><span class="Tit1">Atenci&oacute;n : </span>@fe.DescTar</td>
            </tr>
        </table><br>
        <table width="100%" border="1" cellspacing="0" cellpadding="3">
            <tr>
                <th height="20" colspan="3" align="center">ANAMNESIS</th>
            </tr>
            <tr>
                <td width="35" height="20" valign="top" class="Tit1">Tiempo Enferm.: @fe.TiempoEnfermedad</td>
                <td width="30%" valign="top" class="Tit1">Inicio : @fe.Inicio</td>
                <td width="35%" class="Tit1">Curso : @fe.Curso</td>
            </tr>
            <tr>
                <td height="100" colspan="3" valign="top">
                    <span class="Tit1">Enfermedad Actual :</span><br>
                    @fe.Relato
                </td>
            </tr>
        </table><br>


        <table width="100%" border="1" cellspacing="0" cellpadding="3">
            <tr>
                <th height="20" colspan="6" align="center" bgcolor="#0099FF">ANTENCEDENTES</th>
            </tr>
            <tr>
                <td width="10%" height="20" align="right" valign="top" class="Tit1">Personales : </td>
                <td colspan="5">@ViewBag.antecendenteP</td>
            </tr>
            <tr>
                <td height="20" align="right" valign="top" class="Tit1">Familiares : </td>
                <td colspan="5">@ViewBag.antecendenteF</td>
            </tr>
            <tr>
                <td rowspan="2" align="right" valign="top" class="Tit1">Ginecolog. :</td>
                <td width="18%" height="20" class="Tit1">Paridad : @fe.paridad</td>
                <td width="18%" class="Tit1">PIG : @fe.PIG</td>
                <td width="18%" class="Tit1">FUR : @fe.FUR</td>
                <td width="18%" class="Tit1">FPP : @fe.FPP</td>
                <td width="18%" class="Tit1">Edad Gestac. : @fe.EdadGestacional</td>
            </tr>
            <tr>
                <td height="20" class="Tit1">Ultim. PAP : @fe.UltimoPap</td>
                <td colspan="2" class="Tit1">Resultado : @fe.ResultPap</td>
                <td class="Tit1">MAC TIPTIME : @fe.MACtipTime</td>
                <td></td>
            </tr>
        </table><br>
        <table width="100%" border="1" cellspacing="0" cellpadding="3">
            <tr>
                <th height="20" colspan="8" align="center">EXAMEN FISICO</th>
            </tr>
            <tr>
                <td width="10%" height="20"><span class="Tit1">FC :</span> @fe.FC LPM</td>
                <td width="10%"><span class="Tit1">FR :</span> @fe.FR RPM</td>
                <td width="12%"><span class="Tit1">PA :</span> @fe.PA mmHg</td>
                <td width="12%"><span class="Tit1">T&deg; Axilar :</span> @fe.Tax &deg;C</td>
                <td width="12%" class="Tit1">T&deg; Anal : @fe.Tanal</td>
                <td width="10%"><span class="Tit1">Peso :</span> @fe.Peso Kg</td>
                <td width="10%"><span class="Tit1">Talla :</span> @fe.talla M</td>
                <td width="24%"><span class="Tit1">IMC : </span>@fe.IMC - 
                @if(fe.IMC < 18){ Write("PESO BAJO");}
                else if(fe.IMC < 25){ Write("NORMAL"); }
                else if(fe.IMC < 27){ Write("SOBREPESO"); }
                else if(fe.IMC < 30){ Write("OBESIDAD GRADO I"); }
                else if(fe.IMC < 40){ Write("OBESIDAD GRADO II"); }
                else if(fe.IMC > 40){ Write("OBESIDAD GRADO III");}
                </td>
            </tr>
            <tr>
                <td height="80" colspan="8" valign="top">EXAMEN GENERAL : @fe.ExaGeneral</td>
            </tr>
        </table>
        <br>


}


        <table width="100%%" border="1" cellspacing="0" cellpadding="3">
            <tr>
                <th width="50%" height="20">IMPRESION DIAGNOSTICA</th>
                <th width="50%">EXAMENES  AUXILIARES Y PROCEDIMIENTOS</th>
            </tr>
            <tr>
                <td height="140" valign="top">
                    @foreach (var fe in listaDiagnostico)
                    {
                        var demo = fe.Item + ".- " + fe.CIe10 + " " + fe.Descripcion;
                        @demo <br />
                    }
                </td>
                <td height="70" valign="top">
                    @foreach (var fe in listaExamen)
                    {
                        var demo = fe.Item + ".- " + fe.DescrEspecialidad + " / " + fe.DescTar;
                        @demo <br />
                    }

                </td>
            </tr>
        </table>
        <br>
        <table width="100%" border="1" cellpadding="3" cellspacing="0">
            <col width="95">
            <col width="87">
            <col width="83">
            <col width="74">
            <col width="71">
            <col width="83">
            <col width="73">
            <col width="83" span="2">
            <tr>
                <th width="85%" height="20" align="center">TRATAMIENTO</th>
                <th width="15%" align="center">PROXI. CITA :</th>
            </tr>
            <tr>
                <td height="20" align="center"><strong>Medicamento / Forma Farmaceu. / Conecntración. / Dosis / Frecuencia / Vía / Duración / Observaciones</strong></td>
                <td align="center">@ViewBag.proxCita</td>
            </tr>
            <tr>
                <td rowspan="2" valign="top">
                    @foreach (var fe in listaReceta)
                    {
                        var demo = fe.Item + ".- " + fe.DesFromFar + " / " + fe.Concentra + " / " + fe.Dosis + " / " + fe.DesFre + " / " + fe.ViaAdmin + " / " + fe.Duracion + " / " + fe.Observ;
                        @demo <br />
                    }
                </td>
                <th height="20" align="center">INTERCONSULTA :</th>
            </tr>
            <tr>
                <td width="10%" height="35" align="center">@ViewBag.InterCon</td>
            </tr>
        </table>
        <br>
        <table width="100%" border="0" cellspacing="0" cellpadding="3">
            <tr>
                <td width="75%" height="110" align="center">&nbsp;</td>
                <td width="25%" align="center" valign="bottom" class="Tit1">
                    <p style="border-top:1px #000 solid; padding-top:10px">@ViewBag.abrev @ViewBag.medico<br>
                    SERVICIO DE @ViewBag.servicio</p>
                    
                </td>
            </tr>
        </table>
        <br /><br /><br /><br />
        <small>Historia: @ViewBag.historiaPac</small>

    </div>
</body>
</html>

